{
    "manifest_version": 3,
    "name": "Facebook Group UID Extractor",
    "version": "1.0",
    "description": "Extract Facebook Group UIDs from URLs and export to CSV.",
    "permissions": [
        "downloads"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
}